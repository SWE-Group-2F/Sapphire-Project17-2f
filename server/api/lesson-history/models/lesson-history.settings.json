{
  "kind": "collectionType",
  "collectionName": "lesson_histories",
  "info": {
    "name": "Lesson History",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "number": {
      "type": "decimal",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "expectations": {
      "type": "text"
    },
    "activities": {
      "collection": "activity",
      "via": "lesson_history"
    },
    "unit": {
      "model": "unit",
      "via": "lesson_histories"
    },
    "standards": {
      "type": "string",
      "unique": true
    },
    "link": {
      "type": "string"
    },
    "lesson_module": {
      "model": "lesson-module",
      "via": "lesson_histories"
    }
  }
}
